{
  "name": "fit-fusion",
  "version": "1.0.0",
  "private": true,
  "packageManager": "npm@10.7.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build --filter=apps/web",
    "lint": "turbo run lint",
    "start": "turbo run start --filter=apps/web"
  },
  "devDependencies": {
    "turbo": "2.5.4",
    "typescript": "^5.3.3"
  }
}


# Stripe Configuration
# Get these from your Stripe Dashboard: https://dashboard.stripe.com/apikeys

# Publishable Key (safe to use in mobile apps)
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key_here

# Optional: Stripe Account ID (for multi-account setups)
EXPO_PUBLIC_STRIPE_ACCOUNT_ID=

# App Configuration
EXPO_PUBLIC_APP_NAME=Fit Fusion AI
EXPO_PUBLIC_APP_URL_SCHEME=fitfusionai

# Backend API URL (for production)
EXPO_PUBLIC_API_URL=https://your-api-domain.com

# Development
EXPO_PUBLIC_DEV_MODE=true









